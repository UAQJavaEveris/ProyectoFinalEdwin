function nav(dir){
	window.location.href = dir;
}

function eliminarPersonaje(id){
	window.location.href = "/FrontOperativo/DelUpServlet?id=" + id + "&opcion=" + "eliminar";
	
}

function modificarPersonaje(id){
	window.location.href = "/FrontOperativo/DelUpServlet?id=" + id + "&opcion=" + "modificar";
}